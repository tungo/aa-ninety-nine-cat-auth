<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>


<header>
<% if current_user %>
  <%= current_user.username %> logged in
  <%= button_to "Log out", session_url(current_user), method: :delete %>
<% else %>
  <%= link_to "log in", new_session_url %>
<% end %>

</header>
<%= yield %>
</body>
</html>
